{
  "name": "ReactNativeBoilerplate",
  "version": "0.0.1",
  "private": true,
  "keywords": [
    "react",
    "react-native",
    "js",
    "graphql",
    "redux"
  ],
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "build": "babel src/ -d lib/",
    "prepublish": "npm run build",
    "flow": "flow",
    "bundle-ios": "node node_modules/react-native/local-cli/cli.js bundle --entry-file index.ios.js --bundle-output iOS/main.jsbundle --platform 'ios' --assets-dest ./assets  --dev false --reset-cache",
    "bundle-android": "react-native bundle --dev false --entry-file index.android.js --bundle-output android/app/src/main/assets/index.android.bundle --platform android",
    "port-reset": "adb reverse tcp:8081 tcp:8080 && adb shell input keyevent 82",
    "reload": "adb shell input keyevent 82",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "reset": "watchman watch-del-all && npm cache clean",
    "uninstall": "rm -rf node_modules && npm install",
    "enable-remotedev-debugger": "remotedev-debugger --hostname localhost --port 5678 --injectserver",
    "internet-on": "sudo ifconfig en0 up",
    "internet-off": "sudo ifconfig en0 down",
    "test": "jest",
    "lint": "./node_modules/.bin/eslint . --ext .js",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "apollo-client": "^2.0.4",
    "apollo-client-preset": "^1.0.5",
    "apollo-link-context": "^1.0.3",
    "apollo-link-error": "^1.0.3",
    "apollo-link-http": "^1.3.2",
    "apollo-link-retry": "^2.1.1",
    "apollo-link-ws": "^1.0.4",
    "browser-bunyan": "^1.2.2",
    "crypto-js": "^3.1.9-1",
    "graphql": "^0.12.3",
    "graphql-tag": "^2.6.1",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "mongoid-js": "^1.1.3",
    "prop-types": "^15.6.0",
    "react": "16.0.0-alpha.12",
    "react-apollo": "^2.0.4",
    "react-mixin": "^4.0.0",
    "react-native": "0.45.1",
    "react-native-animatable": "^1.2.4",
    "react-native-device-info": "^0.10.2",
    "react-native-fcm": "^11.1.0",
    "react-native-geocoder": "^0.4.6",
    "react-native-in-app-notification": "^2.1.0",
    "react-native-interactable": "^0.1.7",
    "react-native-loading-spinner-overlay": "^0.5.2",
    "react-native-permissions": "^1.0.6",
    "react-native-redux-toast": "^1.0.1",
    "react-native-scrollable-tab-view": "^0.8.0",
    "react-native-vector-icons": "^4.5.0",
    "react-navigation": "^1.0.0-beta.22",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-persist": "^5.4.0",
    "redux-promise-middleware": "^5.0.0",
    "redux-side-effect": "^2.1.2",
    "redux-thunk": "^2.2.0",
    "rn-sliding-up-panel": "^1.1.7",
    "subscriptions-transport-ws": "^0.9.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-jest": "22.0.4",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react-native": "4.0.0",
    "bunyan": "^1.8.12",
    "eslint": "^4.14.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-flowtype": "^2.40.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-react-native": "^3.2.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.62.0",
    "husky": "^0.14.3",
    "jest": "22.0.4",
    "lint-staged": "^6.0.0",
    "prettier": "^1.9.2",
    "react-devtools": "^3.0.0",
    "react-test-renderer": "16.0.0-alpha.12",
    "redux-devtools": "^3.4.1",
    "redux-devtools-extension": "^2.13.2",
    "remote-redux-devtools": "^0.5.12",
    "remote-redux-devtools-on-debugger": "^0.8.3",
    "remotedev-rn-debugger": "^0.8.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
